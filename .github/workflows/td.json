{
  "containerDefinitions": [
    {
      "name": "strapi-let",
      "image": "strapi:latest",
      "cpu": 0,
      "portMappings": [
        {
          "name": "strapi-let-80-tcp",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [],
      "environmentFiles": [],
      "mountPoints": [],
      "volumesFrom": [],
      "ulimits": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/",
          "awslogs-create-group": "true",
          "awslogs-region": "us-west-1",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "systemControls": []
    }
  ],
  "family": "let-task",
  "executionRoleArn": "arn:aws:iam::533266978173:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "1024",
  "memory": "3072",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
